{
    "configurations": [
        {
            "name": "Launch Program",
            "program": "${workspaceFolder}/bin/uclash.js",
            "args": [
                "generate 0"
            ],
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "Launch exec Program",
            "program": "${workspaceFolder}/bin/uclash.js",
            "args": [
                "exec",
                "0"
            ],
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "find proxy ",
            "program": "${workspaceFolder}/bin/uclash.js",
            "args": [
                "find"
            ],
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "serve",
            "program": "${workspaceFolder}/bin/uclash.js",
            "args": [
                "serve"
            ],
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",            
            "preLaunchTask": "preserve"
        },
    ]
}